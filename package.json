{
  "name": "sjba-backend",
  "version": "1.0.0",
  "description": "Backend API for SJBA website with secure public endpoints",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch server.ts",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "seed": "tsx scripts/seed.ts",
    "seed:clear": "tsx scripts/seed.ts clear",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "express",
    "api",
    "sjba",
    "backend",
    "typescript"
  ],
  "author": "SJBA",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "nodemailer": "^7.0.12",
    "pino": "^9.9.4",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.1"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.9",
    "@types/nodemailer": "^7.0.5",
    "eslint": "^9.0.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.x"
  }
}
