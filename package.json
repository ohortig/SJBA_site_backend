{
  "name": "sjba-backend",
  "version": "1.0.0",
  "description": "Backend API for SJBA website with secure public endpoints",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch server.ts",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "seed": "tsx scripts/seed.ts",
    "seed:clear": "tsx scripts/seed.ts clear",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "express",
    "api",
    "sjba",
    "backend",
    "typescript"
  ],
  "author": "SJBA",
  "license": "MIT",
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@supabase/supabase-js": "^2.39.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "pino": "^9.9.4",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.1",
    "resend": "^6.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/mailchimp__mailchimp_marketing": "^3.0.22",
    "@types/node": "^25.0.9",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "supertest": "^7.1.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=22.x"
  }
}
